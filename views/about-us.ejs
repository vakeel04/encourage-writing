  <script src=" https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

  <script>
    const processSwiper = new Swiper('#process-slider', {
      loop: true,
      slidesPerView: 3, // ek time par 3 cards
      spaceBetween: 20,
      slidesPerGroup: 1,
      navigation: {
        nextEl: '#process-slider .swiper-button-next',
        prevEl: '#process-slider .swiper-button-prev',
      },
      breakpoints: {
        0: {
          slidesPerView: 1.3
        }, // Mobile
        768: {
          slidesPerView: 2
        }, // Tablet
        1024: {
          slidesPerView: 3.8
        } // Desktop
      }
    });
  </script>


